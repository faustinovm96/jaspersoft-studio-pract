	<parameter name="factura" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT fact.detalles_facturacion.facturacion_id_facturacion,
	fact.detalles_facturacion.productos_id_producto,
	fact.detalles_facturacion.cantidad,
	fact.detalles_facturacion.precio,
	fact.productos.id_producto,
	fact.productos.nombre,
	fact.productos.iva,
	if(fact.productos.iva = 'EXENTA', fact.detalles_facturacion.precio * fact.detalles_facturacion.cantidad, ' ') as exenta,
	if(fact.productos.iva = '5', fact.detalles_facturacion.precio * fact.detalles_facturacion.cantidad, ' ') as cinco,
	if(fact.productos.iva = '10', fact.detalles_facturacion.precio * fact.detalles_facturacion.cantidad, ' ') as diez
FROM fact.detalles_facturacion
	INNER JOIN fact.productos ON 
	 fact.detalles_facturacion.productos_id_producto = fact.productos.id_producto
	 WHERE fact.detalles_facturacion.facturacion_id_facturacion =  $P{factura} ]]>
	</queryString>
